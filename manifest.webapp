{
  "name": "Marquee HTML5",
  "description": "Una aplicacion Demo de desarrollo para Firefox OS!!",
  "launch_path": "/marquee.html",
  "default_locale": "es",
  "developer": {
    "name": "Miguel Angel Velazco",
    "url": "https://twitter.com/MikeVelazco"
  },
  "permissions": {
    "desktop-notification": {
      "description": "To show notifications"
    },
    "geolocation": {
      "description": "Marking out user location"
    },
    "alarms": {
      "description": "Scheduling alarms"
    },
    "camera": {
      "description": "Required for accessing cameras on the device."
    }
  },
  "icons": {
    "16": "/images/logo16.png",
    "32": "/images/logo32.png",
    "48": "/images/logo48.png",
    "60": "/images/logo60.png",
    "64": "/images/logo64.png",
    "90": "/images/logo90.png",
    "120": "/images/logo120.png",
    "128": "/images/logo128.png"
  }
}